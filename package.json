{
  "name": "svg-icon-library-starter",
  "version": "1.0.0",
  "description": "Starter project to build an SVG icon library",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "serve": "npm-run-all -p watch-showcase serve-html",
    "serve-html": "http-server ./showcase -o",
    "watch-showcase": "tsc-bundle showcase/main.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kreuzerk/svg-icon-library-starter.git"
  },
  "keywords": [
    "SVG",
    "icons",
    "library",
    "starter"
  ],
  "author": "Kevin Kreuzer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kreuzerk/svg-icon-library-starter/issues"
  },
  "homepage": "https://github.com/kreuzerk/svg-icon-library-starter#readme",
  "devDependencies": {
    "git-cz": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3",
    "typescript-bundle": "^1.0.16"
  }
}
